<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.myweatherforecast.ActivityCityManagement"
        android:orientation="vertical"
        android:background="@android:color/background_light">

    <Button
            android:text="中国省份"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/chinaProvince" android:layout_marginTop="2dp"
            android:textAlignment="viewStart" android:paddingLeft="20dp"
            android:background="@drawable/button_change"/>
    <ExpandableListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/provicen"
            android:layout_marginTop="5dp"/>
</LinearLayout>
